{
  "version": "0.0.0",
  "name": "Bridge",
  "manifest_version": 2,
  "description": "Bridge",
  "browser_action": {
    "default_icon": "img/logo/48x48.png",
    "default_title": "Bridge",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/logo/16x16.png",
    "48": "img/logo/48x48.png",
    "128": "img/logo/128x128.png",
    "scalable": "img/logo/scalable.ico"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
  ],
  "permissions": [ "contextMenus", "tabs", "storage", "http://localhost:3000/", "https://cdn.embedly.com/", "https://platform.twitter.com/" ],
  "content_security_policy": "script-src 'self' https://cdn.embedly.com https://platform.twitter.com; object-src 'self'; style-src * 'unsafe-inline'; img-src 'self' https://i-cdn.embed.ly;"
}
