{
  "manifest_version": 2,
  "name": "{APP_NAME}",
  "description": "{APP_DESCRIPTION}",
  "version": "1.0.11",
  "applications": {
    "gecko": {
      "id": "{2070efa6-8cd3-4318-b3d3-ff4c87ea564c}",
      "strict_min_version": "42.0"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "{APP_DESCRIPTION}"
    }
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "{APP_ID}19-out.png",
    "default_title": "{APP_NAME}"
  },
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "https://*/*",
      "http://*/*"
    ],
    "js": ["content.js"]
  }],
  "icons": {
    "128": "{APP_ID}128.png",
    "48": "{APP_ID}48.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "storage",
    "webRequest",
    "contextMenus",
    "https://fonts.googleapis.com/",
    "{apiUrl}"
  ],
  "web_accessible_resources": ["popup.html"],
  "externally_connectable": {
    "matches": ["{webUrl}"]
  }
}
