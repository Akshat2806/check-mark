{
  "manifest_version": 2,
  "name": "Check",
  "description": "Verify breaking news online",
  "version": "1.0",
  "browser_action": {
    "default_icon": "check19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "check128.png",
    "48": "check48.png"
  },
  "permissions": [
    "activeTab",
    "https://fonts.googleapis.com/"
  ]
}
