{
  "manifest_version": 2,
  "name": "{APP_NAME}",
  "description": "{APP_DESCRIPTION}",
  "version": "1.0.7",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "{APP_DESCRIPTION}"
    }
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "{APP_ID}19-out.png",
    "default_popup": "popup.html",
    "default_title": "{APP_NAME}"
  },
  "icons": {
    "128": "{APP_ID}128.png",
    "48": "{APP_ID}48.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "storage",
    "webRequest",
    "contextMenus",
    "https://fonts.googleapis.com/",
    "{apiUrl}"
  ]
}
