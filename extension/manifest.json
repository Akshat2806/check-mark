{
  "manifest_version": 2,
  "name": "Check-Mark",
  "version": "1.0",

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "images/check.png",
      "96": "images/check.png"
    },
    "default_title": "Check Mark",
    "default_popup": "popup.html"
  },

  "permissions": ["activeTab"]
}
